package ex06;

import java.util.Scanner;

public class asd {

	public static void main(String[] args) {
		
		Scanner scan = new Scanner(System.in);
		int x;
		int n;
		
		System.out.print("몇단?");
		n = scan.nextInt();
		
		System.out.printf("\n%d 단 시작 \n",n);
		for(int i = 1; i<10; i++ ) 
			System.out.println(n + "x" + i+ " = " +n*i);
		
		System.out.printf("\n%d 단 끝", n);
		
	}
}
