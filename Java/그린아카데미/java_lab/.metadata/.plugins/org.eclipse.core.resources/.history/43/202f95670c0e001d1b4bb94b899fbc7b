package ex05;

//연산자
public class OperEx01 {

	public static void main(String[] args) {		
		//산술 연산자
		System.out.println(1+1);
		System.out.println(1-1);
		System.out.println(2*2);
		System.out.println(4/2);
		System.out.println(3%2);
		System.out.println();
		System.out.println(10/3);
		// 자바는 몫을 구하는 연산자가 없다.
	}

}
