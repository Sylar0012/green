package ex06;

import java.util.Scanner;

public class asd {

	public static void main(String[] args) {
		
		Scanner scan = new Scanner(System.in);
		int x;
		int menu;
		
		System.out.print("몇단?");
		menu = scan.nextInt();
		
		System.out.printf("%d 단 시작 \n",menu);
		for(int i = 1; i<10; i++ ) 
			System.out.println(menu + "x" + i+ " = " +menu*i);
		
		System.out.printf("%d 단 끝", menu);
		
	}
}
